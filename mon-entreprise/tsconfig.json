{
	"extends": "../tsconfig.json",
	"compilerOptions": {
		"baseUrl": "source",
		"paths": {
			"Actions/*": ["actions/*"],
			"Components": ["components"],
			"Components/*": ["components/*"],
			"Images/*": ["images/*"],
			"Reducers/*": ["reducers/*"],
			"Selectors/*": ["selectors/*"],
			"Types/*": ["types/*"]
		},
		"noEmit": true,
		"strict": true
	},
	"include": ["types", "source"]
}
